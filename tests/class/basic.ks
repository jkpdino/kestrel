// Basic class declarations

// Simple class without visibility modifier
class Foo { }

// Class with public visibility
public class Bar { }

// Class with private visibility
private class Baz { }

// Class with internal visibility
internal class Qux { }

// Class with fileprivate visibility
fileprivate class Quux { }

// Multiple classes in one file
class First { }
class Second { }
class Third { }
