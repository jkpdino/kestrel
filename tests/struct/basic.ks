// Basic struct declarations

// Simple struct without visibility modifier
struct Foo { }

// Struct with public visibility
public struct Bar { }

// Struct with private visibility
private struct Baz { }

// Struct with internal visibility
internal struct Qux { }

// Struct with fileprivate visibility
fileprivate struct Quux { }

// Multiple structs in one file
struct First { }
struct Second { }
struct Third { }
